package Weather;

import com.github.stuxuhai.jpinyin.PinyinFormat;
import com.github.stuxuhai.jpinyin.PinyinHelper;

import SideBar.Setting;

public class ToPinyin {
	public static String toPinyin() {
		String py = PinyinHelper.convertToPinyinString(Setting.Slocation, ",", PinyinFormat.WITHOUT_TONE);
		return py;
	}
}
